<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fan.dao.OtherDao">
    <select id="findAll" resultType="com.fan.pojo.Other">
        select * from other
    </select>
    <select id="findByTime" resultType="com.fan.pojo.Other">
        select * from other where createTime = #{createTime}
    </select>
</mapper>